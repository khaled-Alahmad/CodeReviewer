# Production Environment Variables for HestiaCP
# نسخ هذا الملف إلى .env وتعديل القيم

NODE_ENV=production
PORT=5000

# Database Configuration - PostgreSQL
# استبدل هذه القيم بمعلومات قاعدة البيانات الخاصة بك
DATABASE_URL=postgresql://username:password@localhost:5432/database_name

# Session Secret (غيّر هذا إلى قيمة عشوائية آمنة)
SESSION_SECRET=your-secure-random-secret-key-here
